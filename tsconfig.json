{
    "compilerOptions": {
        "module": "es2015",
        "baseUrl": "./",
        "outDir": "build",
        "target": "es6",
        "allowJs": true,
        "checkJs": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "lib": ["es2020"],
        "forceConsistentCasingInFileNames": true,
    },
    "include": ["index.ts", "app/**/*.ts"],
    "exclude": ["./webui"]
}